{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "includeFiles": "**"
        }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/favicon\\.(ico|svg)",
            "dest": "/public/favicon.$1"
        },
        {
            "src": "/apple-touch-icon\\.png",
            "dest": "/public/apple-touch-icon.png"
        },
        {
            "src": "/logo\\.svg",
            "dest": "/public/logo.svg"
        },
        {
            "src": "/robots\\.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "buildCommand": "npm install && npm run build",
    "installCommand": "npm install"
}